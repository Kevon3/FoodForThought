@page
@model FoodForThoughtWeb.Pages.Account.ProfileModel
@{
	ViewData["Title"] = "Profile";
}
 <h1>@ViewData["Title"]</h1>


<div class="row m-2">
	<div class="col-2">
		<label>Username: </label>
	</div>
	<div class="col-10">
		<label @Model.profile.Username></label>
	</div>
</div>

<div class="row m-2">
	<div class="col-2">
		<label>First Name: </label>
	</div>
	<div class="col-10">
		<label @Model.profile.FirstName></label>
	</div>
</div>
<div class="row m-2">
	<div class="col-2">
		<label>Last Name: </label>
	</div>
	<div class="col-10">
		<label @Model.profile.LastName></label>
	</div>
</div>
<div class="row m-2">
	<div class="col-2">
		<label>Email: </label>
	</div>
	<div class="col-10">
		<label @Model.profile.Email></label>
	</div>
</div>


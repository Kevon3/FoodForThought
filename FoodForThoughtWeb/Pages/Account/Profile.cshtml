@page
@model FoodForThoughtWeb.Pages.Account.ProfileModel
@{
	ViewData["Title"] = "Profile";

	Layout = HttpContext.User.Identity.IsAuthenticated ? "_LayoutAuthenticated" : "_Layout";
}
 <h1>@ViewData["Title"]</h1>


<div class="row m-2">
	<div class="col-2">
		<label asp-for="profile.Username"></label>
	</div>
	<div class="col-10">
		<label>@Model.profile.Username</label>
	</div>
</div>

<div class="row m-2">
	<div class="col-2">
		<label asp-for="profile.FirstName"></label>
	</div>
	<div class="col-10">
		<label>@Model.profile.FirstName</label>
	</div>
</div>
<div class="row m-2">
	<div class="col-2">
		<label asp-for="profile.LastName"></label>
	</div>
	<div class="col-10">
		<label>@Model.profile.LastName</label>
	</div>
</div>
<div class="row m-2">
	<div class="col-2">
		<label asp-for="profile.Email"></label>
	</div>
	<div class="col-10">
		<label>@Model.profile.Email</label>
	</div>
</div>

